{
  "env": {
    "es2021": true,
    "node": true,
    "jest": true
  },
  "extends": ["eslint:recommended", "plugin:import/recommended", "prettier"],
  "parserOptions": { "ecmaVersion": 2021, "sourceType": "module" },
  "rules": {
    "no-console": ["off"],
    "import/no-unresolved": ["off"],
    "import/named": ["off"]
  }
}

